 "font_size": 14,
 "html": "<!DOCTYPE html>\n<html>\n<head>\n    <meta charset=\"utf-8\">\n    <style>\n        @page {\n            size: A4 landscape;\n            margin: 0.5in;\n        }\n        @media print {\n            body { -webkit-print-color-adjust: exact; print-color-adjust: exact; }\n            table { page-break-inside: avoid; }\n        }\n        body {\n            font-family: Arial, sans-serif;\n            font-size: 10px;\n            margin: 0;\n            padding: 10px;\n        }\n        table.outer {\n            width: 100%;\n            border-collapse: collapse;\n            border: 2px solid black;\n        }\n        table.inner {\n            width: 100%;\n            border-collapse: collapse;\n        }\n        td, th {\n            border: 1px solid black;\n            padding: 2px 4px;\n            font-size: 9px;\n            vertical-align: top;\n        }\n        .header-cell { background-color: #f0f0f0; font-weight: bold; text-align: center; }\n        .number-cell { text-align: right; }\n        .center-cell { text-align: center; }\n        .bold { font-weight: bold; }\n        .no-border { border: none; }\n    </style>\n</head>\n<body>\n    <table class=\"outer\">\n        <!-- Header -->\n        <tr>\n            <td colspan=\"12\" class=\"header-cell\">\n                {% set finished_item = doc.items | selectattr('is_finished_item', 'equalto', 1) | first %}\n                {% if finished_item %}\n                    <span class=\"bold\">{{ finished_item.item_code | upper }}</span>\n                    {% if finished_item.item_name and finished_item.item_name != finished_item.item_code %}\n                        {{ finished_item.item_name | upper }}\n                    {% endif %}\n                {% else %}\n                    <span class=\"bold\">FINISHED PRODUCT</span>\n                {% endif %}\n            </td>\n        </tr>\n\n        <tr>\n            <td colspan=\"6\" valign=\"top\">\n                <table class=\"inner\">\n                    <tr><td colspan=\"6\" class=\"header-cell bold\">RAW MATERIAL DETAILS</td></tr>\n                    <tr>\n                        <td class=\"bold\">RM</td>\n                        <td class=\"bold\">WEIGHT</td>\n                        <td class=\"bold\">ADJ.</td>\n                        <td class=\"bold\">%AGE</td>\n                        <td class=\"bold\">UNIT COST</td>\n                        <td class=\"bold\">AMOUNT</td>\n                    </tr>\n                    {% for item in doc.items %}\n                    <tr>\n                        <td>{{ item.item_code }}</td>\n                        <td class=\"number-cell\">{{ \"%.2f\"|format(item.qty or 0) }}</td>\n                        <td class=\"number-cell\">{{ \"%.2f\"|format(item.qty or 0) }}</td>\n                        <td class=\"number-cell\">{{ \"%.2f\"|format((item.qty or 0) / (doc.total_outgoing_value or 1) * 100) }}</td>\n                        <td class=\"number-cell\">{{ \"%.2f\"|format(item.basic_rate or 0) }}</td>\n                        <td class=\"number-cell\">{{ \"%.2f\"|format((item.qty or 0) * (item.basic_rate or 0)) }}</td>\n                    </tr>\n                    {% endfor %}\n                    {% if not doc.items %}\n                    <tr><td colspan=\"6\" style=\"text-align:center;\">No Raw Material Details</td></tr>\n                    {% endif %}\n                    \n                    <!-- Tinting Items under RM -->\n                    <tr><td colspan=\"6\" class=\"header-cell bold\">TINTING ITEMS</td></tr>\n                    <tr>\n                        <td class=\"bold\">TINT ITEM</td>\n                        <td class=\"bold\">TINT QTY</td>\n                        <td colspan=\"4\"></td>\n                    </tr>\n                    {% for t in doc.custom_tinting_items %}\n                    <tr>\n                        <td>{{ t.tint_item }}</td>\n                        <td class=\"number-cell\">{{ \"%.2f\"|format(t.tint_qty or 0) }}</td>\n                        <td colspan=\"4\"></td>\n                    </tr>\n                    {% endfor %}\n                    {% if not doc.custom_tinting_items %}\n                    <tr><td colspan=\"6\" style=\"text-align:center;\">No Tinting Items</td></tr>\n                    {% endif %}\n\n                    <!-- Filling Details under Tinting -->\n                    <tr><td colspan=\"6\" class=\"header-cell bold\">FILLING DETAILS</td></tr>\n                    <tr>\n                        <td class=\"bold\">BULK ITEM</td>\n                        <td class=\"bold\">FILLED ITEM</td>\n                        <td class=\"bold\">PACK SIZE</td>\n                        <td class=\"bold\">NO. OF PACKS</td>\n                        <td class=\"bold\">TOTAL QTY</td>\n                        <td class=\"bold\">WAREHOUSE</td>\n                    </tr>\n                    {% for f in doc.custom_filling_details %}\n                    <tr>\n                        <td>{{ f.bulk_item }}</td>\n                        <td>{{ f.filled_item }}</td>\n                        <td class=\"center-cell\">{{ f.pack_size }}</td>\n                        <td class=\"number-cell\">{{ f.filled }}</td>\n                        <td class=\"number-cell\">{{ \"%.2f\"|format(f.total_qty or 0) }}</td>\n                        <td>{{ f.target_warehouse }}</td>\n                    </tr>\n                    {% endfor %}\n                    {% if not doc.custom_filling_details %}\n                    <tr><td colspan=\"6\" style=\"text-align:center;\">No Filling Details</td></tr>\n                    {% endif %}\n                    \n                    <tr><td>&nbsp;</td><td>&nbsp;</td><td>&nbsp;</td><td>&nbsp;</td><td>&nbsp;</td><td>&nbsp;</td></tr>\n                </table>\n            </td>\n            <td colspan=\"6\" valign=\"top\">\n                <table class=\"inner\">\n                    <tr><td colspan=\"5\" class=\"header-cell bold\">MACHINE USAGE</td></tr>\n                    <tr>\n                        <td class=\"bold\">DATE</td>\n                        <td class=\"bold\">UNIT</td>\n                        <td class=\"bold\">ON</td>\n                        <td class=\"bold\">OFF</td>\n                        <td class=\"bold\">TOTAL HRS</td>\n                    </tr>\n                    <tr><td>&nbsp;</td><td>&nbsp;</td><td>&nbsp;</td><td>&nbsp;</td><td>&nbsp;</td></tr>\n                    <tr><td>&nbsp;</td><td>&nbsp;</td><td>&nbsp;</td><td>&nbsp;</td><td>&nbsp;</td></tr>\n                    \n                    <!-- Transfer Usage under Machine Usage -->\n                    <tr><td colspan=\"5\" class=\"header-cell bold\">TRANSFER USAGE</td></tr>\n                    <tr>\n                        <td class=\"bold\">DATE</td>\n                        <td class=\"bold\">BX.NO</td>\n                        <td class=\"bold\">QUANTITY</td>\n                        <td class=\"bold\">AUTHORITY</td>\n                        <td>&nbsp;</td>\n                    </tr>\n                    <tr><td>&nbsp;</td><td>&nbsp;</td><td>&nbsp;</td><td>&nbsp;</td><td>&nbsp;</td></tr>\n                    <tr><td>&nbsp;</td><td>&nbsp;</td><td>&nbsp;</td><td>&nbsp;</td><td>&nbsp;</td></tr>\n\n                    <!-- Product Specifications -->\n                    <tr><td colspan=\"5\" class=\"header-cell bold\">PRODUCT SPECIFICATIONS</td></tr>\n                    <tr>\n                        <td class=\"bold\">SPECS</td>\n                        <td class=\"bold\">ATTRIBUTE</td>\n                        <td class=\"bold\">ACTUAL</td>\n                        <td class=\"bold\">REMARK</td>\n                        <td>&nbsp;</td>\n                    </tr>\n                    <tr><td>SG</td><td>&nbsp;</td><td>&nbsp;</td><td>&nbsp;</td><td>&nbsp;</td></tr>\n                    <tr><td>VISCOSITY</td><td>&nbsp;</td><td>&nbsp;</td><td>&nbsp;</td><td>&nbsp;</td></tr>\n                    <tr><td>PH</td><td>&nbsp;</td><td>&nbsp;</td><td>&nbsp;</td><td>&nbsp;</td></tr>\n                    <tr><td>FINISH</td><td>&nbsp;</td><td>&nbsp;</td><td>&nbsp;</td><td>&nbsp;</td></tr>\n                    <tr><td>SHADE</td><td>&nbsp;</td><td>&nbsp;</td><td>&nbsp;</td><td>&nbsp;</td></tr>\n                    <tr><td>GLOSS</td><td>&nbsp;</td><td>&nbsp;</td><td>&nbsp;</td><td>&nbsp;</td></tr>\n                </table>\n            </td>\n        </tr>\n\n        <!-- Efficiency Section -->\n        <tr>\n            <td colspan=\"12\">\n                <table class=\"inner\">\n                    <tr><td colspan=\"8\" class=\"header-cell bold\">EFFICIENCY / TINTER</td></tr>\n                    <tr>\n                        <td>EXPECTED YIELD</td><td>&nbsp;</td>\n                        <td>COLOUR APPROVAL</td><td>&nbsp;</td>\n                        <td>FILLED QUANTITY</td><td>&nbsp;</td>\n                        <td>FINAL APPROVAL</td><td>&nbsp;</td>\n                    </tr>\n                    <tr>\n                        <td>ACTUAL YIELD</td><td>&nbsp;</td>\n                        <td>EFFICIENCY (%)</td><td>&nbsp;</td>\n                        <td>COST</td><td>&nbsp;</td>\n                        <td>COST PER LITRE</td><td>&nbsp;</td>\n                    </tr>\n                    <tr>\n                        <td>TOTAL COST PER KG</td><td>&nbsp;</td>\n                        <td>TOTAL LTRAGE COST</td><td>&nbsp;</td>\n                        <td>TOTAL PACKING COST</td><td>&nbsp;</td>\n                        <td>TOTAL CONV. COST</td><td>&nbsp;</td>\n                    </tr>\n                    <tr>\n                        <td>TOTAL</td><td>&nbsp;</td>\n                        <td>AV. PRODUCT COST</td><td>&nbsp;</td>\n                        <td colspan=\"4\">&nbsp;</td>\n                    </tr>\n                </table>\n            </td>\n        </tr>\n\n        <!-- Footer (signatures) -->\n        <tr>\n            <td colspan=\"12\" style=\"padding:12px; text-align:center;\">\n                Prepared By: __________ &nbsp;&nbsp;&nbsp; Checked By: __________ &nbsp;&nbsp;&nbsp; Approved By: __________\n            </td>\n        </tr>\n    </table>\n</body>\n</html>",
